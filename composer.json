{
    "name": "vendor/mphp-flickr-photo-search",
    "description": "Package for accessing the Flickr flickr.photos.search api",
    "type": "library",
    "homepage": "https://github.com/monkeyphp/mphp-flickr-photo-search",
    "license": "MIT",
    "keywords": [
        "Flickr",
        "photos"
    ],
    "authors": [
        {
            "name": "David White",
            "email": "git@monkeyphp.com",
            "role": "Developer"
        }
    ],
    "support": {
